/* card-posture */
.card-posture{
  position: relative;
  padding: 0;
  width: 100%;
  height:252rpx;
}
.posture-text{
  position: absolute;
  bottom: 10rpx;
  right: 50rpx;
  z-index: 2;
}
.posture-bg{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  height: 100%;
}
.ec-canvas {
  width: 100%;
  height: 100%;
}


.img{
  width: 100%;
}